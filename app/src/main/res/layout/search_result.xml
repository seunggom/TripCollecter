<?xml version="1.0" encoding="utf-8"?>
<android.support.v7.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
                                    xmlns:app="http://schemas.android.com/apk/res-auto"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content" android:id="@+id/search_result_cardview"
                                    app:cardBackgroundColor="@color/com_facebook_button_login_silver_background_color"
                                    app:cardCornerRadius="10dp" android:clickable="true"
                                    app:cardElevation="20dp" android:layout_margin="2dp">
    <View
            android:id="@+id/divider10"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="?android:attr/listDivider"
    />
    <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
        <TextView
                android:text="TextView"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" android:id="@+id/srText_regions" android:textSize="18sp"
                android:layout_margin="2dp" android:paddingLeft="5dp"/>
        <TextView
                android:text="TextView"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" android:id="@+id/srText_title" android:textSize="24sp"
                android:layout_margin="2dp" android:paddingLeft="5dp"/>
        <TextView
                android:text="TextView"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" android:id="@+id/srText_uploadDate" android:layout_margin="2dp"
                android:paddingLeft="5dp"/>
        <RatingBar
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" android:id="@+id/sr_ratingBar"
                style="@android:style/Widget.DeviceDefault.Light.RatingBar.Small" android:numStars="5"
                android:rating="0" android:layout_marginBottom="2dp" android:layout_marginEnd="2dp"
                android:layout_marginLeft="2dp" android:layout_marginRight="2dp" android:layout_marginStart="7dp"
                android:layout_marginTop="2dp"/>
    </LinearLayout>
</android.support.v7.widget.CardView>
